<% @radiosets.each do |radioset| %>
  <div>
    <h4><%=radioset.resident.name%> <%= link_to "See profile", resident_path(radioset.resident), class:"btn btn-primary"%></h4>
    <iframe width="560" height="315" src="<%=radioset.audio_link%>" title="Video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
  </div>
<% end %>
